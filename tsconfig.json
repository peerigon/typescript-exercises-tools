{
    "compilerOptions": {
        "outDir": "dist",
        "target": "ESNext",
        "module": "commonjs",
        "strict": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "declaration": true,
        "lib": [
            "scripthost",
            "es5",
            "es2015",
            "es2016",
            "es2017",
            "es2018",
            "es2019",
            "es2020"
        ]
    },
}
